<Window x:Class="lab17_rabbit_wpf_website.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab17_rabbit_wpf_website"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>

        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>

        </Grid.RowDefinitions>
        <Button x:Name="rabbitButton" Background="Brown" Grid.Row="0" Grid.Column="0" Click="RabbitButton_Click"/>
        <ListBox x:Name="rabbitOutput" Background="Beige" Grid.Row="0" Grid.Column="1" Grid.RowSpan="3" Grid.ColumnSpan="2"/>
        <Image x:Name="rabbitPic" Margin="29,49,37.8,33" Grid.RowSpan="2" Source="Image1.png" Stretch="Fill" Grid.Row="1" />
    </Grid>
</Window>
